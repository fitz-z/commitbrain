{
  "name": "commitbrain",
  "version": "1.0.11",
  "description": "commitbrain is a CLI terminal tool that utilizes chatGPT to automatically generate intelligent commit messages for GitHub repositories",
  "main": "dist/index.js",
  "type": "module",
  "repository": "handipriyono/commitbrain",
  "files": [
    "dist"
  ],
  "bin": "dist/index.mjs",
  "keywords": [
    "ai",
    "git",
    "commit",
    "ai commit",
    "commit ai",
    "chatgpt commit",
    "commitbrain",
    "automatic git commit"
  ],
  "scripts": {
