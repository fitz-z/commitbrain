    "lint": "eslint --cache .",
    "type-check": "tsc"
  },
  "author": "Handi",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
